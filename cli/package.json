{
  "name": "@cellm/cli",
  "version": "1.1.0",
  "description": "CLI tool for CELLM - Spec-Driven Development System for AI Agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "cellm": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cellm",
    "claude",
    "llm",
    "spec-driven",
    "ai-agents",
    "cli"
  ],
  "author": "Murillo Dutt <dev@cellm.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/murillodutt/cellm.git",
    "directory": "cli"
  },
  "homepage": "https://cellm.ai",
  "bugs": {
    "url": "https://github.com/murillodutt/cellm/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^14.0.2",
    "gray-matter": "^4.0.3",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.17",
